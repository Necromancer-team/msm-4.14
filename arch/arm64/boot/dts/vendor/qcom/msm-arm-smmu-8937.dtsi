			qcom,secure-context;
			interrupts = <0 58 0>, <0 58 0>;
			qcom,ctx-num = <6>;
			qcom,iommu-ctx-sids = <0x90c>;
			qcom,iommu-sid-mask = <0x220>;
			label = "venus_sec_bitstream";
			qcom,report-error-on-fault;
